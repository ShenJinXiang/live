<!doctype html>
<html>
<head>
	<meta charset='utf-8'>
	<title>user</title>
</head>
<body>
<h1>USER</h1>
<p><%= title %></p>
<table>
	<tr>
		<th>姓名</th>
		<th>年龄</th>
		<th>地址</th>
	</tr>
	<% for (var i = 0; i < userList.length; i++) { %>
		<tr>
			<td><%= userList[i].name %></td>
			<td><%= userList[i].age %></td>
			<td><%= userList[i].address %></td>
		</tr>
	<% } %>
</table>
</body>
</html>

